# Orchestrator Agent
ORCHESTRATOR_AGENT_SEED=your-orchestrator-agent-seed-phrase
ORCHESTRATOR_PORT=8200

# Connector Agent
CONNECTOR_AGENT_SEED=your-connector-agent-seed-phrase
CONNECTOR_PORT=8199
CONNECTOR_HOST=127.0.0.1
CONNECTOR_KEY_DIR=./keys
CONNECTOR_USER_ID=u_dev
CONNECTOR_DEVICE_ID=dev_local

# Network (testnet or mainnet)
AGENT_NETWORK=testnet

# Agentverse API Key (get from https://agentverse.ai/profile/api-keys)
AGENTVERSE_API_KEY=your-agentverse-api-key

# ASI:One LLM (get from https://asi1.ai)
ASI_ONE_API_KEY=your-asi-one-api-key
ASI_ONE_BASE_URL=https://api.asi1.ai/v1
ASI_ONE_MODEL=asi1

# Demo / Testing
DEMO_PROJECTS_DIR=./demo_projects

# Mailbox (set to true so ASI:One can reach the agent)
USE_MAILBOX=true

# Logging
LOG_LEVEL=INFO
